  ipgui::add_param $IPINST -name "DATA_SIZE" -parent ${Page_0}
  ipgui::add_param $IPINST -name "PRESCALER_MAX" -parent ${Page_0}
  ipgui::add_param $IPINST -name "RAM_DEPTH" -parent ${Page_0}


}

